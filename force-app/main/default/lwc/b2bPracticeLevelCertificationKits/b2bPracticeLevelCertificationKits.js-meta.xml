	<masterLabel>B2B Practice Level Certification Kits</masterLabel>
	<targets>
		<target>lightning__HomePage</target>
		<target>lightningCommunity__Page</target>
		<target>lightningCommunity__Default</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightningCommunity__Default">
			<property name="buttonLabel" type="String" label="Claim Kit Button Label" default="Claim Practice Level Certification Kit"/>
			<property name="shippingAddressLabel" type="String" label="Shipping Address Heading" default="Shipping Address"/>
			<property name="editAddressButtonLabel" type="String" label="Edit Address Button Label" default="Edit Address"/>
			<property name="newAddressButtonLabel" type="String" label="New Address Button Label" default="Add Shipping Address"/>
			<property name="shipMyKitButtonLabel" type="String" label="Ship My Kit Button Label" default="Ship My Kit"/>
