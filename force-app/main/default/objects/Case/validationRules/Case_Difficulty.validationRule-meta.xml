<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Difficulty</fullName>
    <active>false</active>
    <errorConditionFormula>AND (
				IsClosed = TRUE,
				ISPICKVAL(How_Difficult_Was_This_Case_to_Close__c, &apos;&apos;),
				$User.Id != &apos;005Hs00000CWUPD&apos;
				)</errorConditionFormula>
